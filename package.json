{
  "name": "@carbon/vue",
  "license": "Apache-2.0",
  "version": "3.11.0",
  "description": "A collection of components for the Carbon Design System built using Vue.js",
  "packageManager": "yarn@3.2.0",
  "main": "dist/carbon-vue-3.umd.min.js",
  "types": "src/index.d.ts",
  "web-types": [
    "dist/web-types.json",
    "./carbon-web-components-web-types.json"
  ],
  "type": "module",
  "scripts": {
    "postinstall": "node prepare.js",
    "postinstall_prod": "ibmtelemetry --config=telemetry.yml",
    "prepack": "echo prepack",
    "postpack": "echo postpack",
    "dev": "vite",
    "build": "vite build",
    "build-storybook": "storybook build",
    "preview": "vite preview",
    "build-web-types": "vue-docgen-web-types --outFile dist/web-types.json",
    "add-components-exports": "node addNamedComponentExports.cjs",
    "ci-check": "yarn add-components-exports && yarn format:diff && yarn test && yarn build && yarn build-web-types",
    "format": "prettier --write '**/*.{scss,css,js,md,vue}' '!**/{build,es,lib,storybook,ts,umd,.coverage}/**'",
    "format:diff": "prettier --list-different '**/*.{scss,css,js,md,vue}' '!**/{dist,storybook-static,.coverage}/**'",
    "format:staged": "prettier --write '**/*.{scss,css,js,md,vue}' '!**/{dist,storybook-static,.coverage}/**'",
    "lint:css": "stylelint '{packages,storybook}/**/*.{vue,scss}'",
    "lint:es": "eslint src/**/*.{js,vue}",
    "lint:style": "vue-cli-service lint:style",
    "storybook": "storybook dev -p 6006",
    "test": "vue-cli-service test:unit --no-coverage"
  },
  "dependencies": {
    "@carbon/icons-vue": "^10.88.0",
    "@carbon/layout": "^11.21.0",
    "@carbon/web-components": "^2.8.0",
    "@ibm/telemetry-js": "^1.5.0",
    "@storybook/manager-api": "^8.0.9",
    "@storybook/theming": "^8.0.9",
    "@vueuse/core": "^10.9.0",
    "carbon-components": "^11.55.0",
    "flatpickr": "^4.6.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@storybook/vue3": "^8.0.9",
    "@storybook/vue3-vite": "^8.0.9",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "@vue/vue3-jest": "^29.2.6",
    "accessibility-checker": "^3.1.69",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lerna": "^8.1.2",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.19",
    "prettier": "^3.2.5",
    "sass": "^1.56.1",
    "storybook": "^8.0.9",
    "stylelint": "^14.15.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue": "^3.4.23",
    "vue-docgen-web-types": "^0.1.8",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.0.13"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/src",
    "telemetry.yml"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{scss,css,js,md,vue}": [
      "yarn format:staged"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbon-design-system/carbon-components-vue.git"
  },
  "sideEffects": true
}
